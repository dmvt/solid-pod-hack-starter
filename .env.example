# Environment Variables for Solid Pod Hack Frontend
# Copy this file to .env.local for local development

# Solid Pod Server URL (used for authentication and data storage)
VITE_SOLID_POD_SERVER=http://localhost:3000

# Application Mode
VITE_MODE=development

# API Request Timeout (milliseconds)
VITE_API_TIMEOUT=30000

# Application Name
VITE_APP_NAME="Solid Pod Hack"