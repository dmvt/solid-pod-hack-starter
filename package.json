    "jest": "^29.7.0",
    "postcss": "^8.4.32",
    "svelte": "^5.0.0-next.1",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  },
  "dependencies": {
